generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id         Int       @id @default(autoincrement())
  name       String?   @unique
  email      String?   @unique
  password   String
  createTime DateTime? @default(now()) @map("create_time")
  updateTime DateTime? @updatedAt @map("update_time")

  @@map("user")
}
